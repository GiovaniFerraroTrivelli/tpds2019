<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dominio.Descuento" table="descuentos">

		<id name="idDescuento" type="java.lang.Integer"
			column="id_descuento">
			<generator class="native" />
		</id>

		<property name="concepto" column="concepto" />
		<property name="factor" column="factor" />


	</class>
</hibernate-mapping>